{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["post"],
      "route": "createSpec/{version}/{revision}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "name": "outputblob",
      "type": "blob",
      "dataType": "string",
      "path": "api-gen/{version}-{revision}.json",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ]
}
